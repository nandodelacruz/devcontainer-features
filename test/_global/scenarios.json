{
    "color_and_hello": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "color": {
                "favorite": "purple"
            },
            "hello": {
                "greeting": "Greetings"
            }
        }
    },
    "p10k": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "p10k": {
                "config": "~/.p10k.zsh",
                "plugins": "aws git"
            }
        }
    }
}